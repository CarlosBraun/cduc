<style>
  .body {
    background: white;
    background-color: white !important; 

  }
  .container2{
    margin-top:1%;
    margin-left: 0;
    margin-right: 0;
    width: 100%;
    height: auto;
    border-bottom: 1px solid black; 
  }
  .container1{
    margin-top:1%;
    margin-left: 1%;
    margin-right: 1%;
    width: 98%;
  }

  #cborde{
    width: 100%;
